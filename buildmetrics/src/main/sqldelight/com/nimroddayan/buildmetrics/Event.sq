CREATE TABLE event (
  timestamp INTEGER NOT NULL PRIMARY KEY,
  is_success INTEGER AS Boolean DEFAULT 0 NOT NULL,
  duration_seconds INTEGER NOT NULL,
  free_ram TEXT NOT NULL,
  swap_ram TEXT NOT NULL
);

insert:
INSERT INTO event (timestamp, is_success, duration_seconds, free_ram, swap_ram)
VALUES (?, ?, ?, ?, ?);

selectAll:
SELECT * FROM event;

delete:
DELETE FROM event
WHERE timestamp = ?;

purge:
DELETE FROM event;
